  <SQLQuery><![CDATA[SELECT B.BRAND_NAME||' ,'||I.DESCRIPTION MODEL, J.JOBCARDNO, J.CUSTOMER ,j.CHARGES, j.EPCOST, j.JobStatus ,j.jobdate ,j.warranty,J.Prev_DocType, J.Prev_DocId   
FROM SRV_JOBCARD J, SO_PRODUCT_BRANDS B, IN_ITEMS I     
WHERE  J.BRANDID = B.PBRANDID      
AND  J.ITEMID = I.ITEMID     
AND  J.POSTED = 'Y'     
AND J.JOBSTATUS IN('RW','RW2')     
AND J.LOCATIONID = :P_ADF_LOCATIONID     
AND J.JOBCARDNO NOT IN(	select JOBCARDNO from SRV_REVIEWJOBWARRANTY where posted = 'N')     
ORDER BY TO_NUMBER(SUBSTR(J.JOBCARDNO,-8))]]></SQLQuery>
