  <SQLQuery><![CDATA[SELECT  PO.POId, GRN.GRN_ID RNoteno, s.supp_name, grn.grn_date  ,      
        S.SUPPLIERID,      
        S.CGLCode,      
        S.CCostCenter,      
        GRN.REMARKS  ,  
        PO.PAYMENTTERMID,  
        PO.DELIVERYTERMID     
  
FROM SRV_GRN GRN, PU_PURCHASE_ORDERS PO, pu_suppliers s         
WHERE PO.POID = GRN.POID and NVL(GRN.Posted, 'N') = 'Y'         
and po.supplierid = s.supplierid         
AND GRN.LOCATIONID = :P_ADF_LOCATIONID         
AND GRN.GRN_ID Not IN (select PSI.RNOTENO from PU_SUPPLIER_INVOICES PSI Where NVL(PSI.po_for, 'N') = 'P' AND PSI.RNOTENO IS NOT NULL)]]></SQLQuery>
