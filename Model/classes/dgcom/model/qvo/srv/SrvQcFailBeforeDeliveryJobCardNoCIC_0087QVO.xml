  <SQLQuery><![CDATA[SELECT B.BRAND_NAME||' ,'||I.DESCRIPTION MODEL, J.JOBCARDNO, J.IMEI_NO ,J.JOBDATE ,j.jobstatus ,J.Prev_DocType, J.Prev_DocId  
FROM SRV_JOBCARD J, SO_PRODUCT_BRANDS B, IN_ITEMS I    
WHERE J.BRANDID = B.PBRANDID     
  AND J.ITEMID = I.ITEMID    
  AND J.POSTED = 'Y'    
  AND J.JOBSTATUS IN('CO','RD')    
AND J.JOBCARDNO NOT IN (SELECT I.JOBCARDNO FROM SRV_JOBQC I       WHERE I.POSTED = 'N')    
AND J.JOBCARDNO NOT IN (SELECT I.JOBCARDNO FROM SRV_SETDELIVERY I WHERE I.POSTED = 'N')    
AND J.JOBCARDNO NOT IN (SELECT I.JOBCARDNO FROM SRV_INVOICE I WHERE I.POSTED = 'N')    
AND J.JOBCARDNO NOT IN (SELECT D.JOBCARDNO FROM SRV_COURIERDELI M, SRV_CDDETAIL D WHERE M.CDID = D.CDID AND M.POSTED = 'N')    
  AND J.LOCATIONID = :P_ADF_LOCATIONID    
ORDER BY TO_NUMBER(SUBSTR(J.JOBCARDNO,-8))]]></SQLQuery>
