<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SoProdLocPriceProductLocationWiseQVO"
  Version="12.1.2.66.68"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="dgcom.model.qvo.inv.SoProdLocPriceProductLocationWiseQVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="P_ADF_SR_DATE"
    Kind="where"
    Type="oracle.jbo.domain.Date"/>
  <Variable
    Name="P_ADF_ITEMID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="P_ADF_LOCATIONID"
    Kind="where"
    Type="java.lang.String"/>
  <SQLQuery><![CDATA[SELECT B.LOCATIONID, B.DELEAR_PRICE 
    FROM SO_PROD_LOC_PRICE B 
    WHERE B.PRODUCTID = (SELECT I.PRODUCTID FROM IN_ITEMS I WHERE I.ITEMID=:P_ADF_ITEMID) 
      AND B.LOCATIONID = :P_ADF_LOCATIONID 
      AND ((B.EFROM <= :P_ADF_SR_DATE AND B.ETO >= :P_ADF_SR_DATE) OR (B.EFROM <= :P_ADF_SR_DATE AND ETO IS NULL))]]></SQLQuery>
  <ViewAttribute
    Name="Locationid"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="LOCATIONID"
    Expression="LOCATIONID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="15"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="DelearPrice"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="20"
    Scale="6"
    Type="java.math.BigDecimal"
    ColumnType="NUMBER"
    AliasName="DELEAR_PRICE"
    Expression="DELEAR_PRICE"
    SQLType="NUMERIC"/>
  <AttrArray Name="KeyAttributes">
    <Item Value="Locationid"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="dgcom.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
