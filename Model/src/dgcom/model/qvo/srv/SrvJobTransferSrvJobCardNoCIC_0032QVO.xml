  <SQLQuery><![CDATA[SELECT J.JOBCARDNO,b.BRAND_NAME||' ,'||i.description Model, t.description JobType,j.imei_NO  ,j.jobstatus ,j.jobdate      
FROM srv_jobcard j, so_product_brands b, in_items i, SRV_JobTypes t         
where  j.brandid = b.Pbrandid          
and j.jobtypeid = t.jobtypeid         
and j.ItemId = i.ItemId         
and j.posted = 'Y'         
and ( (j.JobStatus in('IN','TL','DAP') AND :P_ADF_DOC_TYPE='JTrans') OR (j.JobStatus in('CO','RD') AND :P_ADF_DOC_TYPE='DPTrans')  )       
and (TECHNICIANID is null)          
and J.jobcardno NOT IN (SELECT CD.jobcardno FROM SRV_JOBTRANSFER C, SRV_JOBTRANSFERDETL CD          
                        WHERE C.TRANSFERID = CD.TRANSFERID AND C.posted = 'N')         
and J.jobcardno NOT IN (select d.JOBCARDNO from SRV_JOBASSIGN m, SRV_JOBASSDETL d where m.ASSIGNID = d.ASSIGNID  and m.posted = 'N')         
and j.locationId = :P_ADF_LOCATIONID         
and NVL(j.FromlocationId,'X') <> :P_ADF_TOLOCATIONID         
order by to_number(substr(j.jobcardno,-8))]]></SQLQuery>
