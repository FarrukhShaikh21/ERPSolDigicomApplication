<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SrvStnPartStnQtyCheckingQueryCIC_0044QVO"
  Version="12.1.2.66.68"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="dgcom.model.qvo.srv.SrvStnPartStnQtyCheckingQueryCIC_0044QVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="P_ADF_FROMSTOREID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="P_ADF_PARTID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="P_ADF_STNDETLID"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="P_ADF_DOC_TYPE_ID"
    Kind="where"
    Type="java.lang.String"
    DefaultValue="STN">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery><![CDATA[SELECT (QTY_ON_HAND - NVL(U.USQTY, 0)) Qty,S.PARTID  
  FROM IN_STORE_PARTS S,  
       (SELECT STOREID, PARTID, SUM(QTY) USQTY  
          FROM VIEW_US_STOCK  
         WHERE STOREID = :P_ADF_FROMSTOREID  
           AND PARTID = :P_ADF_PARTID  
           AND NOT (DOCTYPEID = :P_ADF_DOC_TYPE_ID AND DOCDETLID = NVL(:P_ADF_STNDETLID, 'X'))  
         GROUP BY STOREID, PARTID) U  
 WHERE (S.QTY_ON_HAND - NVL(U.USQTY, 0)) > 0  
   AND S.STOREID = U.STOREID(+)  
   AND S.PARTID = U.PARTID(+)  
   AND S.STOREID = :P_ADF_FROMSTOREID  
   AND S.PARTID = :P_ADF_PARTID]]></SQLQuery>
  <ViewAttribute
    Name="Qty"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="NUMBER"
    AliasName="QTY"
    Expression="QTY"
    SQLType="INTEGER"/>
  <ViewAttribute
    Name="Partid"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="15"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="PARTID"
    Expression="PARTID"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="15"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="Partid"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="dgcom.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
