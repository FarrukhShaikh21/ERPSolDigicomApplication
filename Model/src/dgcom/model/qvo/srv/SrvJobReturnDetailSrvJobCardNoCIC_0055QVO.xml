  <SQLQuery><![CDATA[SELECT J.JOBCARDNO,B.BRAND_NAME||' ,'||I.DESCRIPTION MODEL   ,J.JOBSTATUS ,j.JOBDATE       
FROM SRV_JOBCARD J, SO_PRODUCT_BRANDS B, IN_ITEMS I          
WHERE J.BRANDID = B.PBRANDID           
  AND J.ITEMID = I.ITEMID          
  AND J.POSTED = 'Y'          
  AND ( (J.JOBSTATUS IN( 'RJ' ) AND :P_ADF_DOC_TYPE='JRet') OR (J.JOBSTATUS IN('CP','DP') AND :P_ADF_DOC_TYPE='DPJRet') )      
  AND J.LOCATIONID = :P_ADF_LOCATIONID          
  AND J.FROMLOCATIONID = :P_ADF_TOLOCATIONID          
  AND (J.TECHNICIANID IS NULL)           
 AND J.JOBCARDNO NOT IN (SELECT CD.JOBCARDNO FROM SRV_JOBRETURN C, SRV_JOBRETDETL CD           
                                            WHERE C.JOBRETID = CD.JOBRETID AND C.POSTED = 'N')          
ORDER BY TO_NUMBER(SUBSTR(J.JOBCARDNO,-8))]]></SQLQuery>
